{
  "name": "colisee-builder",
  "version": "0.0.1",
  "description": "Service to build AI clients into Docker images.",
  "main": "dist/src/app.js",
  "scripts": {
    "setup": "npm install",
    "build": "tsc",
    "build-watch": "watch",
    "start-dev": "npm run build && node dist/src/app.js",
    "start": "node src/app.js",
    "start-prod": "NODE_ENV=production node src/app.js",
    "test": "npm run build && mocha test/unit test/integration",
    "clean": "rm -rf src/*.js src/**/*.js test/*.js test/**/*.js node_modules/"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/siggame/colisee-builder.git"
  },
  "author": "siggame",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/siggame/colisee-builder/issues"
  },
  "homepage": "https://siggame.io/colisee-builder",
  "devDependencies": {
    "@types/body-parser": "^1.16.4",
    "@types/chai": "^4.0.3",
    "@types/cors": "^2.8.1",
    "@types/dotenv": "^4.0.0",
    "@types/express": "^4.0.36",
    "@types/http-errors": "^1.5.34",
    "@types/lodash": "^4.14.72",
    "@types/mocha": "^2.2.41",
    "@types/node": "^8.0.20",
    "@types/winston": "^2.3.4",
    "apidoc": "^0.17.6",
    "chai": "^4.1.1",
    "mocha": "^3.5.0",
    "typedoc": "^0.8.0",
    "typescript": "^2.4.2"
  },
  "dependencies": {
    "body-parser": "^1.17.2",
    "cors": "^2.8.4",
    "dotenv": "^4.0.0",
    "express": "^4.15.4",
    "http-errors": "^1.6.2",
    "lodash": "^4.17.4",
    "winston": "^2.3.1"
  }
}
